 */ 

abstract class Animal{
    abstract fun eat()
    
    fun run(){
        println("animal can run")
    }
}

class dog:Animal()
{
    override fun eat(){
        println("dog can run")
    }
}

fun main()
{
    val d=dog()
    d.run()
    d.eat()
    
}
