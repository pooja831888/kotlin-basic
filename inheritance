// inheritance


open class animal(val name:String){
     
    open fun sound()
     {
         print("yes")
     }
 }

class dog (name:String) : animal(name){
    override fun sound()
    {
        print("no")
    }
    
}

fun main()
{
    val d=dog("pooja")
    d.sound()
}




