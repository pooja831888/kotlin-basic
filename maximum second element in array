// maximum second element in array

fun main() {
    
    val arr=intArrayOf(10,20,30,40,50)
    var firstmax=Int.MIN_VALUE
    var secondmax=Int.MIN_VALUE
    
    for(num in arr)
    {
       if(num>firstmax)
        {
           secondmax=firstmax
           firstmax=num
            
        }
        else if(num>secondmax && num!=firstmax)
        {
            secondmax=num
        }
    }
       println(secondmax)

    
    
}
